    <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/retina.min.js"></script>
    <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/justgage.1.0.1.min.js"></script>

    <!-- inline scripts related to this page -->
    <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/pages/index.js"></script>
    <!-- end: JavaScript-->
