{% autoescape false %}
<!DOCTYPE html>
<html lang="pt_BR">
<head>
    <!-- start: Meta -->
    <meta charset="utf-8">
    <title>Administração - {% raw %}{% block titulo %}{% endblock titulo %}{% endraw %} - Login</title>
    <meta name="description" content="{% raw %}{% block descricao %}{% endblock descricao %}{% endraw %}">
    <meta name="author" content="Inkuba">
    <meta name="keyword" content="ACME, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <!-- end: Meta -->

    <!-- start: Mobile Specific -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- end: Mobile Specific -->

    {% raw %}{% block stylesheets %}{% endraw %}
    <!-- start: CSS -->
    <link href="{% raw %}{{ assetUri }}{% endraw %}/web/css/bootstrap.min.css" rel="stylesheet">
    <link href="{% raw %}{{ assetUri }}{% endraw %}/web/css/style.min.css" rel="stylesheet">
    <link href="{% raw %}{{ assetUri }}{% endraw %}/web/css/retina.min.css" rel="stylesheet">
    <!-- end: CSS -->
    {% raw %}{% endblock stylesheets %}{% endraw %}

    <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/respond.min.js"></script>
        <link href="{% raw %}{{ assetUri }}{% endraw %}/web/css/ie.css" rel="stylesheet">
    <![endif]-->

    <!--[if IE 9]>
        <link id="ie9style" href="{% raw %}{{ assetUri }}{% endraw %}/web/css/ie9.css" rel="stylesheet">
    <![endif]-->

    <!-- start: Favicon and Touch Icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{% raw %}{{ assetUri }}{% endraw %}/web/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{% raw %}{{ assetUri }}{% endraw %}/web/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{% raw %}{{ assetUri }}{% endraw %}/web/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="57x57" href="{% raw %}{{ assetUri }}{% endraw %}/web/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="{% raw %}{{ assetUri }}{% endraw %}/web/ico/favicon.png">
    <!-- end: Favicon and Touch Icons -->

    <style type="text/css">body { background: url("{% raw %}{{ assetUri }}{% endraw %}/web/img/bg-login.jpg") !important; }
    </style>
</head>

<body>
        <div class="container">
        <div class="row">

            <div class="row">
                <div class="login-box">
                    <div class="icons">
                        <a href="index.html"><i class="halflings-icon home"></i></a>
                        <a href="login.html#"><i class="halflings-icon cog"></i></a>
                    </div>
                    <h2>Login</h2>
                    {% raw %}
                    {% if error is not empty %}
                    <div class="alert alert-danger">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        {{ error }}
                    </div>
                    {% endif %}
                    {% endraw %}
                    <form class="form-horizontal" action="{% raw %}{{ baseUri }}/{{ url }}{% endraw %}" method="post">
                        <fieldset>
                            <div class="input-wrap">
                            <div class="input-prepend input-group" title="Username">
                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                <input class="form-control" name="usuario" id="username" type="text" placeholder="informe o usuário"/>
                            </div>
                            </div>
                            <div class="input-wrap">
                            <div class="input-prepend input-group" title="Password">
                                <span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                <input class="form-control" name="senha" id="password" type="password" placeholder="informe a senha"/>
                            </div>
                            </div>
                            <label class="remember" for="remember"><input type="checkbox" id="remember" />Lembrar me</label>

                            <div class="button-login">  
                                <button type="submit" class="btn btn-primary">Login</button>
                            </div>
                            <div class="clearfix"></div>
                    </form>
                    <hr>
                    <h3>Esqueceu sua senha?</h3>
                    <p>
                        <a href="{% raw %}{{ baseUri }}{% endraw %}/esqueci-senha">Clique aqui</a> para obter uma nova senha.
                    </p>
                </div><!--/col-->
            </div><!--/row-->

        </div><!--/row-->

        <div class="modal fade" id="myModal">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title">Modal title</h4>
                    </div>
                    <div class="modal-body">
                        <p>Here settings can be configured...</p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        <button type="button" class="btn btn-primary">Save changes</button>
                    </div>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
        
    </div><!--/container-->

    {% raw %}{% block javascripts %}{% endraw %}
        <!-- start: JavaScript-->
        <!--[if !IE]>-->
                <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/jquery-2.1.0.min.js"></script>
        <!--<![endif]-->

        <!--[if IE]>
            <script src="assets/js/jquery-1.11.0.min.js"></script>
        <![endif]-->

        <!--[if !IE]>-->
            <script type="text/javascript">
                window.jQuery || document.write("<script src='{% raw %}{{ assetUri }}{% endraw %}/web/js/jquery-2.1.0.min.js'>"+"<"+"/script>");
            </script>
        <!--<![endif]-->

        <!--[if IE]>
            <script type="text/javascript">
            window.jQuery || document.write("<script src='{% raw %}{{ assetUri }}{% endraw %}/web/js/jquery-1.11.0.min.js'>"+"<"+"/script>");
            </script>
        <![endif]-->
        <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/jquery-migrate-1.2.1.min.js"></script>
        <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/bootstrap.min.js"></script>

        <!-- theme scripts -->
        <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/custom.min.js"></script>
        <script src="{% raw %}{{ assetUri }}{% endraw %}/web/js/core.min.js"></script>
        <!-- end: JavaScript-->
    {% raw %}{% endblock javascripts %}{% endraw %}
</body>
</html>
{% endautoescape %}